	<insert id="userLogAdd" parameterType="com.usersauth.entity.SysUserLog">
		INSERT INTO sys_user_log 
		(user_id,user_name,menu_code,menu_name,permission_code,permission_name,record_date) 
		VALUE
		(#{user_id},
		(SELECT username user_name FROM sys_user where id=#{user_id}),
		#{menu_code},
		(SELECT DISTINCT menu_name FROM sys_permission WHERE menu_code=#{menu_code} LIMIT 0,1),
		#{permission_code},
		(SELECT DISTINCT permission_name FROM sys_permission WHERE permission_code=#{permission_code} LIMIT 0,1),
		#{record_date})
	</insert>
	
